{"ast":null,"code":"/*eslint-disable*/\nimport NavbarHome from \"../components/NavbarHome.vue\";\nimport FooterButtom from \"../components/FooterButtom.vue\";\nimport StepController from \"../components/StepController.vue\";\nimport AddressFrom from \"../components/AddressForm.vue\";\nimport DeliveryForm from \"../components/DeliveryForm.vue\";\nimport PaymentForm from \"../components/PaymentForm.vue\";\nimport ShoppingCart from \"../components/ShoppingCart.vue\";\nexport default {\n  name: 'shopping-home',\n  components: {\n    NavbarHome,\n    FooterButtom,\n    StepController,\n    AddressFrom,\n    DeliveryForm,\n    PaymentForm,\n    ShoppingCart\n  },\n\n  data() {\n    return {\n      step: 1,\n      formStep: [\"Address\", \"Delivery\", \"Payment\"],\n      isFormCompleted: false,\n      deliveryFee: 0,\n      addressData: {},\n      deliveryData: '',\n      PaymentData: {}\n    };\n  },\n\n  methods: {\n    nextStep(step) {\n      this.step = step + 1; // this.$router.push({ name: this.formStep[this.step - 1] });\n    },\n\n    preStep(step) {\n      this.step = step - 1; // this.$router.push({ name: this.formStep[this.step - 1] });\n    },\n\n    confirmAccount() {\n      this.isFormCompleted = true; //待完善：顯示確認訂單頁面\n    },\n\n    handleFeeAmount(feeAmount) {\n      this.deliveryFee = feeAmount;\n    },\n\n    getAddress(address) {\n      this.AddressData = address;\n    },\n\n    getDelivery(delivery) {\n      this.deliveryData = delivery;\n      console.log(this.deliveryData);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;EACAA,qBADA;EAEAC;IACAC,UADA;IAEAC,YAFA;IAGAC,cAHA;IAIAC,WAJA;IAKAC,YALA;IAMAC,WANA;IAOAC;EAPA,CAFA;;EAWAC;IACA;MACAC,OADA;MAEAC,4CAFA;MAGAC,sBAHA;MAIAC,cAJA;MAKAC,eALA;MAMAC,gBANA;MAOAC;IAPA;EASA,CArBA;;EAsBAC;IACAC;MACA,qBADA,CAEA;IACA,CAJA;;IAKAC;MACA,qBADA,CAEA;IACA,CARA;;IASAC;MACA,4BADA,CAEA;IACA,CAZA;;IAaAC;MACA;IACA,CAfA;;IAgBAC;MACA;IACA,CAlBA;;IAmBAC;MACA;MACAC;IACA;;EAtBA;AAtBA","names":["name","components","NavbarHome","FooterButtom","StepController","AddressFrom","DeliveryForm","PaymentForm","ShoppingCart","data","step","formStep","isFormCompleted","deliveryFee","addressData","deliveryData","PaymentData","methods","nextStep","preStep","confirmAccount","handleFeeAmount","getAddress","getDelivery","console"],"sourceRoot":"src/views","sources":["ShoppingHome.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <NavbarHome />\n    <main>\n      <div class=\"container\">\n        <h2 class=\"checkout-title\">結帳</h2>\n      </div>\n\n      <div class=\"container\">\n        <!-- checkout stepper (Left)-->\n        <div class=\"checkout-wrapper\">\n          <!-- step controller -->\n          <StepController :now-step=\"step\" :form-completed=\"isFormCompleted\" />\n\n          <!-- form panel -->\n          <div class=\"form-panel\">\n            <!-- 用v-if呈現 -->\n            <AddressFrom v-if=\"step === 1\" />\n            <DeliveryForm v-if=\"step === 2\" @feeAmount-chosen=\"handleFeeAmount\" />\n            <PaymentForm v-if=\"step === 3\"/>\n\n            <!-- 用router-view -->\n            <!-- <router-view\n              @feeAmount-chosen=\"handleFeeAmount\"\n              @addressForm-saved=\"getAddress\"      \n              \n            /> -->\n          </div>\n\n          <!-- Step control button -->\n          <div class=\"btn-control d-flex justify-content-between\">\n            <button\n              class=\"btn btn-outline\"\n              v-if=\"step > 1\"\n              @click.stop.prevent=\"preStep(step)\"\n            >\n              &larr; 上一步\n            </button>\n            <button\n              class=\"btn btn-primary\"\n              v-if=\"step === 3\"\n              @click.stop.prevent=\"confirmAccount\"\n            >\n              確認訂單\n            </button>\n            <button\n              class=\"btn btn-primary\"\n              v-else\n              @click.stop.prevent=\"nextStep(step)\"\n            >\n              下一步 &rarr;\n            </button>\n          </div>\n        </div>\n\n        <!-- shopping Cart (Right)-->\n        <ShoppingCart :delivery-fee=\"deliveryFee\" />\n      </div>\n    </main>\n    <Footer />\n  </div>\n</template>\n\n<script>\n/*eslint-disable*/\nimport NavbarHome from \"../components/NavbarHome.vue\";\nimport FooterButtom from \"../components/FooterButtom.vue\";\nimport StepController from \"../components/StepController.vue\";\nimport AddressFrom from \"../components/AddressForm.vue\";\nimport DeliveryForm from \"../components/DeliveryForm.vue\";\nimport PaymentForm from \"../components/PaymentForm.vue\";\nimport ShoppingCart from \"../components/ShoppingCart.vue\";\nexport default {\n  name: 'shopping-home',\n  components: {\n    NavbarHome,\n    FooterButtom,\n    StepController,\n    AddressFrom,\n    DeliveryForm,\n    PaymentForm,\n    ShoppingCart,\n  },\n  data() {\n    return {\n      step: 1,\n      formStep: [\"Address\", \"Delivery\", \"Payment\"],\n      isFormCompleted: false,\n      deliveryFee: 0,\n      addressData: {},\n      deliveryData: '',\n      PaymentData: {},\n    };\n  },\n  methods: {\n    nextStep(step) {\n      this.step = step + 1;\n      // this.$router.push({ name: this.formStep[this.step - 1] });\n    },\n    preStep(step) {\n      this.step = step - 1;\n      // this.$router.push({ name: this.formStep[this.step - 1] });\n    },\n    confirmAccount() {\n      this.isFormCompleted = true;\n      //待完善：顯示確認訂單頁面\n    },\n    handleFeeAmount(feeAmount) {\n      this.deliveryFee = feeAmount;\n    },\n    getAddress(address) {\n      this.AddressData = address;\n    },\n    getDelivery(delivery) {\n      this.deliveryData = delivery\n      console.log(this.deliveryData)\n    }\n  },\n};\n</script>\n\n<style lang=\"scss\">\n@import \"../assets/scss/main.scss\";\n</style>"]},"metadata":{},"sourceType":"module"}